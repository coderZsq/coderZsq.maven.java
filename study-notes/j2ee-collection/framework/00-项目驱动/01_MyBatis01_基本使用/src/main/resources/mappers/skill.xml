<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="skill">
    <select id="list" resultType="com.sq.bean.Skill">
        SELECT * FROM skill
    </select>

    <select id="list2" resultType="com.sq.bean.Skill">
        SELECT * FROM skill WHERE id > #{id} OR level > #{level} OR name LIKE #{name}
    </select>

    <select id="get" resultType="com.sq.bean.Skill">
        SELECT * FROM skill WHERE id = #{id}
    </select>

